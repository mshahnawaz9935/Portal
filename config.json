{
  "app":{
    "js": {
        "src" : [          
          "app/src/js/app.js",      
          "app/src/js/*.js"     
        ],
        "dist" :"dist/js/"
    },
    "css": {
      "src" : [
			  "./bower_components/angular-material/angular-material.css",
        "app/src/css/*.css"
      ],
      "dist" : "dist/css/"
    },
    "views": {
      "src": "app/src/views/**/*.html",
      "dist": "dist/views/"
    },
    "fonts": {
      "src": "app/src/assets/fonts/*",
      "dist": "dist/assets/fonts/"
    },
    "icons": {
      "src": "app/src/assets/icons/**/*",
      "dist": "dist/assets/icons/"
    }
 },

 "vendors": [ 
   	"./bower_components/angular/angular.js",
		"./bower_components/angular-material/angular-material.js",
    "./bower_components/angular-animate/angular-animate.js",
    "./bower_components/angular-aria/angular-aria.js",
    "./bower_components/highcharts/highcharts.src.js",
    "http://code.highcharts.com/maps/modules/map.js", 
    "http://code.highcharts.com/mapdata/custom/world.js",
    "./bower_components/highcharts-ng/dist/highcharts-ng.min.js"
  ]
}


